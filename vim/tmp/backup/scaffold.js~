'use strict';
function htmlFrom(templateCacheName){
  var html;
  inject(function($templateCache){
    html = $templateCache.get('index.html');
  });
  return html;
}

function ngFromHtml(html, vm){
  var spa, scope;
  inject(function($compile, $rootScope){
    scope = $rootScope.$new();
    if (vm) { scope.vm = vm; }
    spa = $compile(html)(scope);
    $rootScope.$digest();
  });
  return spa;
}

function ngFrom(templateCacheName, vm){
  return ngFromHtml(htmlFrom(templateCacheName), vm);
}
